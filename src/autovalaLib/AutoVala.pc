prefix=@CMAKE_INSTALL_PREFIX@
real_prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@DOLLAR@{prefix}
libdir=@DOLLAR@{exec_prefix}/${CMAKE_INSTALL_LIBDIR}
includedir=@DOLLAR@{exec_prefix}/${CMAKE_INSTALL_INCLUDEDIR}

Name: AutoVala
Description: AutoVala
Version: 1.0
Libs: -L@DOLLAR@{libdir} -lAutoVala
Cflags: -I@DOLLAR@{includedir}
Requires: cairo gdk-3.0 gdk-pixbuf-2.0 gee-0.8 gio-2.0 glib-2.0 gobject-2.0 libxml-2.0 pango pangocairo
